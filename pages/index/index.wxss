.slider swiper,
.slider image {
  width: 750rpx;
  height: 390rpx;
}
.wrap {
  border-top: 1rpx solid #ccf;
  border-bottom: 1rpx solid #ccf;
  display: flex;
  flex-wrap: wrap;
}
.wrap navigator {
  padding: 10px 0;
  width: 33.33333%;
  display: flex;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ccf;
  border-right: 1rpx solid #ccf;
   flex-direction: column;
  justify-content: center;
  align-items: center; 
}
.wrap navigator:nth-of-type(3n) {
  border-right: none;
}
.wrap navigator image {
  height: 100rpx; 
  width: 100rpx; 
}
.wrap navigator text {
  font-size: 14px;
}
 .commends{
  height:160rpx;
  display: flex; 
  padding: 5rpx;
}
.commends image{
  height: 160rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
} 
.commends image:first-child{
  margin-right: 5rpx;
}